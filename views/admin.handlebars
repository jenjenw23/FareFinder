{{> header}}
<div class="container">  
  <div class="row" style="margin-top: 15px;">
    <div class="col-lg-12 text-center">
      <h2>Admin</h2>
    </div>
  </div>
  <div class="row" style="margin-top: 15px;">
    <div class="col text-center"> 
      <h2>----------</h2>
    </div>
  </div>
  <div class="row"  style="margin-top: 15px;">
    <div class="col-lg-12 text-center">
      <h2>Users</h2>
      <ol>
        <!--move burger and buttons from partials-->
        {{#each users}} 
        <li>{{this.name}}
          <!--needed to be inside the li so show up correctly}-->
          <!--<button class="change-state" data-id="{{id}}" data-newstate="{{devoured}}">
            {{#unless devoured}}Devour!{{/unless}}
          </button>-->
          <a class="btn btn-default" data-id="{{id}}" href="/admin/users/:id" role="button">Edit</a>
        </li>
        {{/each}} 
      </ol>
    </div>
  </div>
</div>
{{> footer}}